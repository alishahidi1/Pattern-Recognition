    "N=1000\n",
    "xh=np.random.randn(2,N);\n",
    "#plt.plot(xh[0,:],xh[1,:],'b.')\n",
    "sig=np.array([[2, -2],\n",
    "              [-2, 3]])\n",
    "mu=np.array([[0],[0]])\n",
    "\n",
    "l, phi=np.linalg.eig(sig)\n",
    "lam=np.diag(l)\n",
    "m1=lam**0.5\n",
    "m2=np.matmul(m1,xh)\n",
    "X=np.matmul(phi,m2)+mu\n",
    "\n",
    "\n",
    "wd=2*m1[0,0]\n",
    "hi=2*m1[1,1]\n",
    "ang=np.arctan(phi[1,0]/phi[0,0])/np.pi*180\n",
    "elps=ptch.Ellipse(mu,wd,hi,ang,facecolor='none',edgecolor='blue',linewidth=2.5)\n",
    "plt.figure(figsize=(10,10))\n",
    "ax=plt.gca()\n",
    "\n",
    "ax.plot(X[0,:],X[1,:],'r.',markersize=3)\n",
    "ax.add_patch(elps)\n",
    "ax.axis('equal');\n",
    "\n",
    "#plt.axis='equal'\n",
    "#ax.set_xlim(-8, 8)\n",
    "#ax.set_ylim(-6, 6)\n",
    "#plt.show()\n",
    "\n",
    "muc = np.reshape(np.mean(X,axis=1),(2,1))\n",
    "myCov=np.matmul((X-muc),np.transpose(X-muc))/(N-1)\n",
    "myCov"
   ]
